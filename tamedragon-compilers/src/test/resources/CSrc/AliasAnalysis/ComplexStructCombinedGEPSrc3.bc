  %7 = getelementptr inbounds %struct.PersonStruct* %6, i32 %personIndex2, i32 2
  %8 = load i32* %7, align 4
  %9 = icmp sgt i32 %4, %8
  br i1 %9, label %10, label %11

; <label>:10                                      ; preds = %0
  br label %12

; <label>:11                                      ; preds = %0
  br label %12

; <label>:12                                      ; preds = %11, %10
  %elder.0 = phi i32 [ 1, %10 ], [ 0, %11 ]
  ret i32 %elder.0
}