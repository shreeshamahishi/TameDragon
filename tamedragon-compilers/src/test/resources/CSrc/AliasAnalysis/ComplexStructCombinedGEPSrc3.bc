define i32 @foo(i32 %compIndex1, i32 %personIndex1, i32 %compIndex2, i32 %personIndex2, %struct.CompanyStruct* %companies) nounwind {
  %1 = getelementptr inbounds %struct.CompanyStruct* %companies, i32 %compIndex1, i32 2
